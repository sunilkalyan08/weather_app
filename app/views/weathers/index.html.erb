<% $localities.each do |data, val| %>
  <input id="<%= "#{val}" %>" type="button" value="<%= "#{data}" %>" class="btn-libra weather-report">
<% end %>
<br><br><br>
<div id="weather-info">
</div>
<script>
  $('.weather-report').click(function(e){
    var data = 'city=' + $(this).val();
    $.ajax({data: data, dataType: 'script', type: 'post', url: '/weather_report'})
  })
</script>
